                    //h3 Hardware Checked Out
                    //h5= x
                    table.table.table-striped.table-bordered.my-4
                        thead
                            tr
                                th(scope='col') ID
                                th(scope='col') Name
                                th(scope='col') Hardware Name
                                th(scope='col') X
                                th(scope='col') X
                                th(scope='col') X
