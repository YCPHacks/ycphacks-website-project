html
  head
    link(rel='icon', type='image/x-icon', href='/static/images/favicon.png')
  body
   include includes/navbar.pug
   include includes/dashboardBar.pug
           .display-5 User List
               hr
           .container
               each user of users
                   img(src=user['picture'] referrerpolicy="no-referrer")

                   each [key, value] of Object.entries(user)
                       unless (key == 'picture')
                           div= `${key}: ${value}`
