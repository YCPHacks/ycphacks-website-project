html
  head
    link(rel='icon', type='image/x-icon', href='/static/images/favicon.png')
  body
   include includes/navbar.pug
  .container-sm
   h1.text-center Users List
   table.table.table-success.table-striped.table-hover.table-bordered
      tr
        each column in columns
          th.table-dark= column
      each row in rows
        tr
          each entry in row
            td= entry