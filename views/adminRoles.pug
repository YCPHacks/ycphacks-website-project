                // PARTICIPANTS LIST
                // SEARCH BAR
                .container
                    table.table.table-success.table-striped.table-bordered.table-responsive
                        thead.table-dark
                            tr
                                th(scope='col') First
                                th(scope='col') Last
                                th(scope='col') Email
                        tbody
                            tr
                                each val in admin_list
                                    td= val







